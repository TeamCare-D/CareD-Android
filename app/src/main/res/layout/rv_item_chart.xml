<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="205dp"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/bar_rv_item_chart_1"
        android:layout_width="8dp"
        android:layout_height="90dp"
        android:background="@color/colorPointBlue"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginBottom="10dp"
        app:layout_constraintBottom_toTopOf="@+id/txt_rv_item_chart_name"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/bar_rv_item_chart_2"
        android:layout_width="8dp"
        android:layout_height="0.001dp"
        android:background="@drawable/chart_yellow_4"
        app:layout_constraintStart_toStartOf="@+id/bar_rv_item_chart_1"
        app:layout_constraintBottom_toTopOf="@+id/bar_rv_item_chart_1"
        />
    <TextView
        android:id="@+id/bar_rv_item_chart_3"
        android:layout_width="8dp"
        android:layout_height="0.001dp"
        android:background="@color/colorYellow"
        app:layout_constraintStart_toStartOf="@+id/bar_rv_item_chart_1"
        app:layout_constraintBottom_toTopOf="@+id/bar_rv_item_chart_2"
        />

    <TextView
        android:id="@+id/txt_rv_item_chart_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:textSize="10sp"
        android:textColor="@color/colorBlack"
        android:lineSpacingExtra="5sp"
        android:text="프로바이오틱스"
        />

    <TextView
        android:layout_width="30dp"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toEndOf="@+id/bar_rv_item_chart_1"
        app:layout_constraintBottom_toBottomOf="@+id/bar_rv_item_chart_1"
        />

</androidx.constraintlayout.widget.ConstraintLayout>