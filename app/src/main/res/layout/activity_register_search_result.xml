<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/cl_register_search_result"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".register.search.RegisterSearchResultActivity">

    <ImageView
        android:id="@+id/btn_register_search_result_back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="6dp"
        app:layout_constraintEnd_toStartOf="@+id/txt_register_search_result_keyword"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/btn_back" />

    <TextView
        android:id="@+id/txt_register_search_result_keyword"
        android:layout_width="0dp"
        android:layout_height="35dp"
        android:layout_marginTop="11dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/grey_fill_18"
        android:fontFamily="@font/notosanskr_regular"
        android:includeFontPadding="false"
        android:lineSpacingExtra="6sp"
        android:paddingStart="12dp"
        android:paddingTop="7dp"
        android:paddingBottom="8dp"
        android:text="비타민"
        android:textColor="@color/colorBlack"
        android:textSize="14dp"
        app:layout_constraintEnd_toStartOf="@+id/btn_register_search_result"
        app:layout_constraintStart_toEndOf="@+id/btn_register_search_result_back"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/btn_register_search_result_delete"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/txt_register_search_result_keyword"
        app:layout_constraintEnd_toEndOf="@+id/txt_register_search_result_keyword"
        app:layout_constraintTop_toTopOf="@+id/txt_register_search_result_keyword"
        app:srcCompat="@drawable/btn_delete" />

    <ImageView
        android:id="@+id/btn_register_search_result"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="6dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/txt_register_search_result_keyword"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/btn_search" />

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="0dp"
        android:layout_height="11dp"
        android:layout_marginTop="17dp"
        android:background="@color/colorLightGray"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txt_register_search_result_keyword" />

    <TextView
        android:id="@+id/txt_register_search_result_count"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginTop="26dp"
        android:text="결과 3건"
        android:fontFamily="@font/notosanskr_bold"
        android:textSize="14dp"
        android:lineSpacingExtra="6sp"
        android:includeFontPadding="false"
        android:textColor="@color/colorBlack"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView14" />

    <ScrollView
        android:id="@+id/scrollView2"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginTop="39dp"
        android:layout_marginBottom="39dp"
        app:layout_constraintBottom_toTopOf="@+id/btn_register_search_result_next"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txt_register_search_result_count">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/rv_register_search_result_list"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>

    <Button
        android:id="@+id/btn_register_search_result_next"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginEnd="28dp"
        android:layout_marginBottom="60dp"
        android:background="@drawable/selector_research_next"
        android:enabled="true"
        android:fontFamily="@font/notosanskr_bold"
        android:includeFontPadding="false"
        android:lineSpacingExtra="6sp"
        android:text="다음으로 넘어가기"
        android:textColor="@color/colorDarkGray"
        android:textSize="14dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="0dp"
        android:layout_height="156dp"
        android:background="@color/colorLightGray"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="25dp"
        android:text="찾으시는 제품이 없으신가요?"
        android:gravity="center"
        android:fontFamily="@font/notosanskr_regular"
        android:textSize="12dp"
        android:lineSpacingExtra="6sp"
        android:includeFontPadding="false"
        android:textColor="@color/colorDarkGray"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView8" />

    <TextView
        android:id="@+id/txt_register_search_result_bottom1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/notosanskr_regular"
        android:gravity="center"
        android:includeFontPadding="false"
        android:lineSpacingExtra="6sp"
        android:text="제품의 성분과 함량을 직접 "
        android:textColor="@color/colorDarkGray"
        android:textSize="12dp"
        app:layout_constraintEnd_toStartOf="@+id/txt_register_search_result_bottom2"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/txt_register_search_result_bottom2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/notosanskr_medium"
        android:includeFontPadding="false"
        android:lineSpacingExtra="6sp"
        android:text="등록"
        android:textColor="@color/colorPointBlue"
        android:textSize="12dp"
        app:layout_constraintStart_toEndOf="@+id/txt_register_search_result_bottom1"
        app:layout_constraintTop_toTopOf="@+id/txt_register_search_result_bottom1"
        app:layout_constraintEnd_toStartOf="@+id/txt_register_search_result_bottom3"/>

    <TextView
        android:id="@+id/txt_register_search_result_bottom3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/notosanskr_regular"
        android:gravity="center"
        android:includeFontPadding="false"
        android:lineSpacingExtra="6sp"
        android:text="해 관리받을 수 있어요"
        android:textColor="@color/colorDarkGray"
        android:textSize="12dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/txt_register_search_result_bottom2"
        app:layout_constraintTop_toTopOf="@+id/txt_register_search_result_bottom2" />

</androidx.constraintlayout.widget.ConstraintLayout>