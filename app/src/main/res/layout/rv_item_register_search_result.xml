<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <ImageView
        android:id="@+id/img_register_search_result"
        android:layout_width="144dp"
        android:layout_height="144dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_product_1" />

    <TextView
        android:id="@+id/txt_register_search_result_brand"
        style="@style/light12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="더리얼"
        app:layout_constraintStart_toStartOf="@+id/img_register_search_result"
        app:layout_constraintTop_toBottomOf="@+id/img_register_search_result" />

    <TextView
        android:id="@+id/txt_register_search_result_overseas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="해외직구"
        android:fontFamily="@font/notosanskr_bold"
        android:textSize="10dp"
        android:lineSpacingExtra="5sp"
        android:includeFontPadding="false"
        android:textColor="@color/colorGrayishBrown1"
        app:layout_constraintEnd_toEndOf="@+id/img_register_search_result"
        app:layout_constraintTop_toBottomOf="@+id/img_register_search_result" />

    <TextView
        android:id="@+id/txt_register_search_result_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="알티지 오메가 3 맥스 1400"
        android:fontFamily="@font/notosanskr_bold"
        android:textSize="12dp"
        android:lineSpacingExtra="8sp"
        android:includeFontPadding="false"
        android:textColor="@color/colorBlack"
        app:layout_constraintStart_toStartOf="@+id/txt_register_search_result_brand"
        app:layout_constraintTop_toBottomOf="@+id/txt_register_search_result_brand" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="8dp"
        android:fontFamily="@font/notosanskr_regular"
        android:includeFontPadding="false"
        android:lineSpacingExtra="6sp"
        android:text="유지일 : "
        android:textColor="@color/colorBlack"
        android:textSize="12dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/txt_register_search_result_name"
        app:layout_constraintTop_toBottomOf="@+id/txt_register_search_result_name" />

    <TextView
        android:id="@+id/txt_register_search_result_day"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="40"
        android:fontFamily="@font/notosanskr_bold"
        android:textSize="12dp"
        android:lineSpacingExtra="6sp"
        android:includeFontPadding="false"
        android:textColor="@color/colorMango"
        app:layout_constraintStart_toEndOf="@+id/textView8"
        app:layout_constraintTop_toTopOf="@+id/textView8" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="일"
        android:fontFamily="@font/notosanskr_regular"
        android:textSize="12dp"
        android:lineSpacingExtra="6sp"
        android:includeFontPadding="false"
        android:textColor="@color/colorBlack"
        app:layout_constraintStart_toEndOf="@+id/txt_register_search_result_day"
        app:layout_constraintTop_toTopOf="@+id/txt_register_search_result_day" />

    <Button
        android:id="@+id/btn_register_search_reuslt_check"
        android:layout_width="28dp"
        android:layout_height="28dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/selector_register_search_result"
        android:enabled="false"
        app:layout_constraintEnd_toEndOf="@+id/img_register_search_result"
        app:layout_constraintTop_toTopOf="@+id/img_register_search_result" />
</androidx.constraintlayout.widget.ConstraintLayout>